/boot				emmc		/dev/block/mmcblk0p27	defaults
/cache				ext4		/dev/block/mmcblk0p34	defaults
/system				ext4		/dev/block/mmcblk0p38	flags=backup=1
/cust				ext4		/dev/block/mmcblk0p39	defaults
/data				ext4		/dev/block/mmcblk0p40	flags=length=-16384;backup=1
/external_sd			auto		/dev/block/mmcblk1p1 	/dev/block/mmcblk1	flags=display="sdcard";storage;wipeingui
/usb				auto		/dev/block/sda1		/dev/block/sda		flags=display="usb storage";storage;wipeingui;removable
